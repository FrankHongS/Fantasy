<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.frankhon.fantasymusic">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name=".Fantasy"
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher_music"
            android:label="@string/app_name"
            android:roundIcon="@drawable/ic_launcher_music_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:networkSecurityConfig="@xml/network_security_config"
            tools:ignore="AllowBackup,GoogleAppIndexingWarning" tools:targetApi="n">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>