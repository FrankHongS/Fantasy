<?xml version="1.0" encoding="utf-8"?>
<com.frankhon.fantasymusic.view.SlidingUpPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/supl_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragment"
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:defaultNavHost="true"
            app:navGraph="@navigation/main_nav_graph" />

    <include
            layout="@layout/layout_panel"
            android:layout_width="match_parent"
            android:layout_height="@dimen/default_panel_total_height" />

</com.frankhon.fantasymusic.view.SlidingUpPanelLayout>
