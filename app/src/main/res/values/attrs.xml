<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PanelLayout">
        <attr name="panelHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AnimatedAudioToggleButton">
        <attr name="tintColor" format="color" />
        <attr name="loadingBarPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AnimatedAudioCircleImageView">
        <attr name="borderWidth" format="dimension" />
        <attr name="borderColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundRectImageView">
        <attr name="radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="GridTextView">
        <attr name="itemLayout" format="reference" />
        <attr name="marginHorizontal" format="dimension" />
        <attr name="marginVertical" format="dimension" />
        <attr name="spanCount" format="integer" />
    </declare-styleable>
</resources>